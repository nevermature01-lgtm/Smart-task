{
  "entities": {
    "UserProfile": {
      "title": "User Profile",
      "description": "Represents a user's profile information stored in Supabase.",
      "type": "object",
      "properties": {
        "id": {
            "type": "string",
            "format": "uuid",
            "description": "The user's unique identifier from Supabase Auth."
        },
        "first_name": {
          "type": "string",
          "description": "The user's first name."
        },
        "last_name": {
          "type": "string",
          "description": "The user's last name."
        },
        "email": {
          "type": "string",
          "format": "email",
          "description": "The user's email address."
        }
      },
      "required": ["id", "email"]
    },
    "Team": {
      "title": "Team",
      "description": "Represents a team of users.",
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "The name of the team."
        },
        "code": {
          "type": "string",
          "description": "A unique 6-digit code to join the team."
        },
        "owner_id": {
          "type": "string",
          "format": "uuid",
          "description": "The UID of the user who created the team."
        }
      },
      "required": ["name", "code", "owner_id"]
    }
  },
  "auth": {
    "providers": [
      "password"
    ]
  }
}
